        "@vitejs/plugin-vue": "^1.2.2",
        "@vue/compiler-sfc": "^3.0.7",
        "autoprefixer": "^10.2.5",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-vue": "^7.9.0",
