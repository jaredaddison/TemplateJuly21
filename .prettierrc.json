{
    "semi": false,
    "jsxSingleQuote": true,
    "plugins": ["eslint"],
    "quoteProps": "as-needed",
    "parser":"vue",
    "vueIndentScriptAndStyle": false,
    "insertPragma": false,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 90,
    "endOfLine": "auto",
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
}